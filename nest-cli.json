{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "plugins": [
      {
        "name": "@nestjs/swagger",
        "options": {
          "dtoFileNameSuffix": "Dto.ts",
          "controllerFileNameSuffix": "Controller.ts",
          "typeFileNameSuffix": "Model.ts"
        }
      }
    ]
  }
}